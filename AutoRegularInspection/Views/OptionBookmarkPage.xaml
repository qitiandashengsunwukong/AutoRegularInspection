<Page x:Class="AutoRegularInspection.Views.OptionBookmarkPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AutoRegularInspection.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="OptionBookmarkPage">

    <StackPanel>
        <Label Content="桥面系书签起始编号" ></Label>
        <TextBox Text="{Binding BridgeDeckBookmarkStartNo,UpdateSourceTrigger=PropertyChanged}" Width="200"></TextBox>
        <Label Content="上部结构书签起始编号" ></Label>
        <TextBox  Text="{Binding SuperSpaceBookmarkStartNo,UpdateSourceTrigger=PropertyChanged}" Width="200"></TextBox>
        <Label Content="下部结构书签起始编号" ></Label>
        <TextBox  Text="{Binding SubSpaceBookmarkStartNo,UpdateSourceTrigger=PropertyChanged}" Width="200"></TextBox>
    </StackPanel>
</Page>
